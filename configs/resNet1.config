{
    "input_transform": [{
        "type": "RandomHorizontalFlip",
        "args": {
            "p": 0.5
        }
    },{
        "type": "RandomRotation",
        "args": {
            "degrees": 30
        }
    },{
        "type": "RandomResizedCrop",
        "args": {
            "size": [224,224]
        }
    },{
        "type": "ToTensor",
        "args": {}
    }],
    "output_transform": [],
    "training": {
        "batch_size": 32,
        "shuffle": true,
        "num_workers": 4,
        "lr": 0.001,
        "momentum": 0.9,
        "plot_loss": true,
        "quiet": false,
        "epochs": 100,
        "optimizer": "Adam",
        "loss": "cross_entropy",
        "weight_decay": 0.0001
    },
    "model":
    {
        "type": "ResidualNetwork",
        "args": {
            "num_classes": 2
        }
    },
    "device": "cuda"
}
