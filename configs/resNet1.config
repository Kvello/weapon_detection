{
    "input_transform": [{
        "type": "RandomHorizontalFlip",
        "args": {
            "p": 0.5
        }
    },{
        "type": "RandomRotation",
        "args": {
            "degrees": 30
        }
    },{
        "type": "RandomResizedCrop",
        "args": {
            "size": [224,224]
        }
    }],
    "output_transform": {
        "type": "softmax"
    },
    "train_config": {
        "batch_size": 32,
        "epochs": 10,
        "learning_rate": 0.001,
        "optimizer": "Adam",
        "loss_function": "cross_entropy"
    },
    "data_augmentation": {
        "horizontal_flip": true,
        "rotation": 15,
        "zoom_range": [0.9, 1.1]
    },
    "regularization": {
        "dropout": 0.5,
        "l2": 0.0005
    }
}
